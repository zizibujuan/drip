<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
<extension
         point="org.eclipse.equinox.http.registry.httpcontexts">
         <httpcontext
            id="com.zizibujuan.drip.client.httpcontext.core">
	         <resource-mapping
	               bundle="com.zizibujuan.drip.client.core"
	               path="/web">
	         </resource-mapping>
	         <resource-mapping
	               bundle="com.zizibujuan.drip.client.mathjax"
	               path="/static">
	         </resource-mapping>
	         <resource-mapping
	               bundle="com.zizibujuan.drip.client.mathjax"
	               path="/static">
	         </resource-mapping>
         </httpcontext>
         <httpcontext
            id="com.zizibujuan.drip.client.httpcontext.dripx">
	         <resource-mapping
	               bundle="com.zizibujuan.drip.client.core"
	               path="/web/dripx">
	         </resource-mapping>
         </httpcontext>
    </extension>
	<extension
         point="org.eclipse.equinox.http.registry.resources">
      <resource
            alias="/"
            httpcontextId="com.zizibujuan.drip.client.httpcontext.core">
      </resource>
      <resource
            alias="/dripx"
            httpcontextId="com.zizibujuan.drip.client.httpcontext.dripx">
      </resource>
     </extension>
</plugin>
