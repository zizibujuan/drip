<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
	<extension
         point="org.eclipse.equinox.http.registry.httpcontexts">
         <httpcontext
            id="com.zizibujuan.drip.client.httpcontext.dojo">
	         <resource-mapping
	               bundle="com.zizibujuan.drip.client.dojo"
	               path="/static/dojo">
	         </resource-mapping>
         </httpcontext>
          <httpcontext
            id="com.zizibujuan.drip.client.httpcontext.dijit">
	         <resource-mapping
	               bundle="com.zizibujuan.drip.client.dojo"
	               path="/static/dijit">
	         </resource-mapping>
         </httpcontext>
          <httpcontext
            id="com.zizibujuan.drip.client.httpcontext.dojox">
	         <resource-mapping
	               bundle="com.zizibujuan.drip.client.dojo"
	               path="/static/dojox">
	         </resource-mapping>
         </httpcontext>
         <httpcontext
            id="com.zizibujuan.drip.client.httpcontext.util">
	         <resource-mapping
	               bundle="com.zizibujuan.drip.client.dojo"
	               path="/static/util">
	         </resource-mapping>
         </httpcontext>
    </extension>
	<extension
         point="org.eclipse.equinox.http.registry.resources">
      <resource
            alias="/dojo"
            httpcontextId="com.zizibujuan.drip.client.httpcontext.dojo">
      </resource>
      <resource
            alias="/dijit"
            httpcontextId="com.zizibujuan.drip.client.httpcontext.dijit">
      </resource>
      <resource
            alias="/dojox"
            httpcontextId="com.zizibujuan.drip.client.httpcontext.dojox">
      </resource>
      <resource
            alias="/util"
            httpcontextId="com.zizibujuan.drip.client.httpcontext.util">
      </resource>
     </extension>
</plugin>
